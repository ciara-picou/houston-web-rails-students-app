<%= form_for (@student) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :major %>
    <%= f.text_field :major %>
    <%= f.label :age %>
    <%= f.number_field :age %>
    <%= f.label "Please select an instructor" %>
    <%= f.collection_select :instructor_id, @instructors, :id, :name %>
    <% if flash[:errors] %>
<% flash[:errors].each do |error| %>
    <li style="color:red"><%= error %></li>
    <% end %>
    <% end %>
    <%= f.submit %>
    <% end %>